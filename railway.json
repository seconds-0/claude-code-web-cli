{
  "$schema": "https://railway.com/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "Dockerfile.web"
  },
  "deploy": {
    "startCommand": "node /app/apps/web/server.js",
    "healthcheckPath": "/",
    "restartPolicyType": "on_failure",
    "restartPolicyMaxRetries": 5
  }
}
